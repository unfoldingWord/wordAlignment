    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "rootDir": "./",
    "coverageDirectory": "coverage",
    "setupFiles": [
      "./testSetUp/shim.js",
      "./testSetUp/enzyme.js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
